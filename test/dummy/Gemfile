source 'https://rubygems.org'
gem 'rails'
group :development, :test do
    gem 'sqlite3'
end
group :production do
    gem 'pg'
end

# Gems used only for assets and not required
# in production environments by default.
group :assets do
  gem 'sass-rails'
  gem 'coffee-rails'
  gem 'therubyracer'
  gem 'uglifier'
end

gem 'bcrypt'
gem 'browser'
gem 'geoip'
gem 'aws-sdk-rails'
gem 'dotenv'
